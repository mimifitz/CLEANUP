CREATE TABLE `services` (
	`id` INT NOT NULL AUTO_INCREMENT,
	`type` TEXT NOT NULL,
	`name` TEXT NOT NULL,
	`description` varchar(255) NOT NULL,
	`availability` DATETIME NOT NULL,
	`location` TEXT NOT NULL,
	`userId` INT NOT NULL,
	PRIMARY KEY (`id`)
);

ALTER TABLE `services` ADD CONSTRAINT `services_fk0` FOREIGN KEY (`userId`) REFERENCES `services`(`id`);



